<%- include('layouts/head.ejs') %>
<html>   
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <title>LinkedItems | Past Orders</title>
    <body>
        <%- include('layouts/navigation.ejs') %>
        <div class="col-md-6">
            <h3 class="text-center">OrdersList</h3>
            <table class="table " id="myTable">
                <thead class="thead-dark">
                    <tr>
                        <th>Product_id</th>
                        <th>Quantity</th>
                        <th>price</th>
                        <th>total</th>
                        <th>Order_date</th>
                    </tr>
                </thead>
                <tbody>
                    <% for (let j = 0; j < data.recordset.length; j++) {%>                    <tr>
                        <td><%= data.recordset[j].Product_id %></td>
                        <td><%= data.recordset[j].Quantity %></td>
                        <td><%= data.recordset[j].price %></td>
                        <td><%= data.recordset[j].total %></td>
                        <td><%= data.recordset[j].Order_date %> </td>
  
                    </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
    </body>


    <%- include('layouts/foot.ejs') %>
